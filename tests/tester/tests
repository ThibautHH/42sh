SETUP=""
TESTS=
./usr/bin/ls -l
[502-END]

[503]
NAME="Execve: /"
SETUP=""
TESTS=
/usr/bin/ls -l
[503-END]

[601]
NAME="Error: command not found"
SETUP=""
TESTS=
noexist
[601-END]

[602]
NAME="Error: command wrong arguments"
SETUP=""
TESTS=
ls nofile
[602-END]

[701]
NAME="Parsing: ;"
SETUP=""
TESTS=
ls -l ; pwd; cat .gitignore
[701-END]

[702]
NAME="Parsing: ; with error"
SETUP=""
TESTS=
ls -l ; noexist; cat .gitignore
[702-END]

[801]
NAME="Redirection: >"
SETUP=""
TESTS=
ls -l > out
cat out; rm out
[801-END]

[802]
NAME="Redirection: >>"
SETUP=""
TESTS=
pwd > out
ls -l >> out
cat out; rm out
[802-END]

[803]
NAME="Redirection: <"
SETUP=""
TESTS=
cat < .gitignore
[803-END]

[804]
NAME="Redirection: | even number of pipes"
SETUP=""
TESTS=
ls -l | cat -e | grep Makefile
[804-END]

[805]
NAME="Redirection: | odd number of pipes"
SETUP=""
TESTS=
ls -l | cat -e | cat -e | cat -e
ls -l | cat -e | cat -e | cat -e
[805-END]

[806]
NAME="Redirection: | with >"
SETUP=""
TESTS=
ls -l | cat -e > out; rm out
[806-END]

[909]
NAME="Segfault program"
SETUP=""
TESTS=
./tests/tester/bin/segf
[909-END]
