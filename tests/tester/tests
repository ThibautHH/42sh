[400]
NAME="Built-in alias with and without arg"
SETUP=""
CLEAN=""
TESTS=
alias oui ls
alias oui
alias
[400-END]

[401]
NAME="Built-in unalias simple"
SETUP=""
CLEAN=""
TESTS=
alias oui ls
unalias oui
alias
[401-END]

[402]
NAME="Built-in unalias multiple"
SETUP=""
CLEAN=""
TESTS=
alias oui ls
alias non ls
alias win ls
unalias oui non win
alias
[402-END]

[403]
NAME="Built-in alias substitution"
SETUP=""
CLEAN=""
TESTS=
alias oui ls
oui
unalias oui
oui
[403-END]

[404]
NAME="Built-in alias complex substitution"
SETUP=""
CLEAN=""
TESTS=
alias oui ls -a
oui ; alias oui cat -e ; ls | oui
ls | oui
[404-END]

[405]
NAME="Built-in simple alias loop"
SETUP=""
CLEAN=""
TESTS=
alias a b
alias b a
a
[405-END]

[406]
NAME="Built-in complex alias loop"
SETUP=""
CLEAN=""
TESTS=
alias a b ; alias b c ; alias c d ; alias d a
a
[406-END]

[407]
NAME="Built-in alias false loop"
SETUP=""
CLEAN=""
TESTS=
alias ls ls -a
ls
[407-END]

