[001]
NAME="Parsing: space and tabs"
SETUP=""
TESTS=
		    ls	     	-l    		  	 -l ..
[001-END]

[101]
NAME="Built-in exit"
SETUP=""
TESTS=
exit
[101-END]

[102]
NAME="Built-in env"
SETUP="export TERM=xterm"
TESTS=
unsetenv _ SHLVL NLSPATH HOSTTYPE VENDOR OSTYPE MACHTYPE GROUP HOST
env
[102-END]

[201]
NAME="Built-in cd"
SETUP=""
TESTS=
pwd
cd src
pwd
[201-END]

[202]
NAME="Built-in cd .."
SETUP=""
TESTS=
pwd
cd ..
pwd
[202-END]

[203]
NAME="Built-in cd -"
SETUP=""
TESTS=
pwd
cd src
pwd
cd -
pwd
[203-END]

[204]
NAME="Built-in cd $HOME"
SETUP=""
TESTS=
pwd
cd
pwd
[204-END]

[205]
NAME="Built-in cd ~"
SETUP=""
TESTS=
pwd
cd ~
pwd
[205-END]

[206]
NAME="Built-in cd /"
SETUP=""
TESTS=
pwd
cd /
pwd
[206-END]

[207]
NAME="Built-in cd nofolder"
SETUP=""
TESTS=
pwd
cd nofolder
pwd
[207-END]

[208]
NAME="Built-in cd file"
SETUP=""
TESTS=
pwd
cd Makefile
pwd
[208-END]

[209]
NAME="Built-in cd to much args"
SETUP=""
TESTS=
pwd
cd src ..
pwd
[209-END]

[301]
NAME="Built-in setenv one arg"
SETUP="export TERM=xterm"
TESTS=
unsetenv _ SHLVL NLSPATH HOSTTYPE VENDOR OSTYPE MACHTYPE GROUP HOST
env
setenv EGAL
env
[301-END]

[302]
NAME="Built-in setenv two arg"
SETUP="export TERM=xterm"
TESTS=
unsetenv _ SHLVL NLSPATH HOSTTYPE VENDOR OSTYPE MACHTYPE GROUP HOST
env
setenv EGAL 1
env
[302-END]

[303]
NAME="Built-in setenv no arg"
SETUP="export TERM=xterm"
TESTS=
unsetenv _ SHLVL NLSPATH HOSTTYPE VENDOR OSTYPE MACHTYPE GROUP HOST
env
setenv
[303-END]

[304]
NAME="Built-in setenv invalid name"
SETUP=""
TESTS=
setenv EGAL=1
[304-END]

[305]
NAME="Built-in setenv too many arguments"
SETUP=""
TESTS=
setenv arg1 arg2 arg3
[305-END]

[400]
NAME="Built-in alias with and without arg"
SETUP=""
CLEAN=""
TESTS=
    echo 'alias oui ls'
    echo 'alias oui'
    echo 'alias'
[400-END]

[401]
NAME="Built-in unalias simple"
SETUP=""
CLEAN=""
TESTS=
    echo 'alias oui ls'
    echo 'unalias oui'
    echo 'alias'
[401-END]

[402]
NAME="Built-in unalias multiple"
SETUP=""
CLEAN=""
TESTS=
    echo 'alias oui ls'
    echo 'alias non ls'
    echo 'alias win ls'
    echo 'unalias oui non win'
    echo 'alias'
[402-END]

[501]
NAME="Execve: path"
