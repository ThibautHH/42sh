SETUP=""
TESTS=
ls -l | cat nofile
[810-END]

[811]
NAME="Piping: exit status propagation"
SETUP=""
TESTS=
exit 45 | ls -l | cat -e
[811-END]

[812]
NAME="Piping: exit status propagation with error"
SETUP=""
TESTS=
exit 45 | ls -l | cat -j | cat
[812-END]

